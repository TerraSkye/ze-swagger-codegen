<?php
declare(strict_types=1);

use Swagger\Middleware\ModelMiddlewareFactory;
use Laminas\ServiceManager\Factory\InvokableFactory;

return [
    'dependencies' => [
        'factories'  => [
            {{#models}}
            {{.}} => ModelMiddlewareFactory::class,
            {{/models}}
        ]
    ],
    'hydrators' => [
        'factories' => [
            {{#hydrators}}
            {{hydrator}} => InvokableFactory::class,
            {{/hydrators}}
        ]
    ]
];
